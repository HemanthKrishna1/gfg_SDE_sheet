class Solution
{
    public:
    //Function is to check whether two strings are anagram of each other or not.
    bool isAnagram(string a, string b){
        int dp[26] = {0};
        for(auto &it: a){
            dp[it-'a']++;
        }
        for(auto &it: b){
            dp[it-'a']--;
        }
        for(auto it: dp){
            if(it!=0){
                return false;
            }
        }
        return true;
        
    }

};
